{
  "manifest_version": 3,
  "name": "Budget Manager",
  "description": "This extension tracks your overall spendings.",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "salary.png"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "eventPage.js"
  },
  "permissions": ["storage", "notifications", "contextMenus"]
}
